/* pages/title/classify/search/search.wxss */
.container_search{
  width: 100%;
  height: 100vh;
  margin: 0;
  padding: 0;
  overflow: hidden; /*防止左右滑动显示白色区域 */
  
}
.v1{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  padding: 0 10rpx 0 20rpx;
}
.v1>input{
  width: 78%;
  height: 60%;
  border: 1rpx solid #ccc;
  border-radius: 26rpx;
  padding:0 20rpx;
  font-size: 28rpx;
  background: url(data:image/svg+xml;base64,) no-repeat 95% 50%;
  background-size: 40rpx;
}
.v1_t{
  width: 10%;
  height: 60%;
  background: url(data:image/svg+xml;base64,) no-repeat 50% 50%;
  background-size: 40rpx;
}
.v2{
  width: 100%;
  height: 100rpx;
  margin: 0;
  display: flex;
}
.v2>text{
  width: 25%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items:  flex-start;
}
.active{
  color: red;
}

.v3{
  width: 100%;
  height: 78%;
  background: #ddd;
  overflow-y:auto;
  display: flex;
  flex-wrap: wrap;
  /* padding-bottom: 200rpx; */
}
.search_list{
  height: 550rpx;
  margin: 0 auto;
  border-radius: 13rpx;
  background: white;
  width: 46%;
  margin: 20rpx 1.5% 0;
}
.v3_image{
  width:100%;
  height: 350rpx;
}
.v3_image>image{
  width: 100%;
  height: 100%;
}
.v3_txt{
  width: 100%;
  height: 40rpx;
  font-size: 32rpx;
  padding: 20rpx 20rpx 10rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.v3_bottom{
  width: 100%;
  height: 120rpx;
  display: flex;
}
.v3_left{
  width: 80%;
  height: 100%;
}
.v3_left>text{
  font-size: 32rpx;
  display: block;
  text-indent: 20rpx;
  width: 100%;
  height: 50%;
  display: flex;
  
}
.v3_t1{
  color: red;
  align-items: flex-end;
  padding: 2rpx 0;
}
.v3_t2{
  font-size: 28rpx;
  color: #ccc;
  align-items: flex-start;
}
.v3_right{
  width: 20%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.v3_right>image{
  width: 70%;
  height: 35%;
}